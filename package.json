{
  "name": "gatsby-starter-gcn",
  "description": "A starter template to build amazing static websites with Gatsby, Contentful and Netlify",
  "version": "2.0.6",
  "repository": "https://github.com/ryanwiemer/gatsby-starter-gcn",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "chalk": "^5.3.0",
    "contentful-import": "^9.4.29",
    "gatsby": "^4.25.8",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^5.13.0",
    "gatsby-plugin-catch-links": "^5.13.0",
    "gatsby-plugin-feed": "^4.25.0",
    "gatsby-plugin-google-analytics": "^4.25.0",
    "gatsby-plugin-manifest": "^5.13.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-nprogress": "^5.13.0",
    "gatsby-plugin-offline": "^5.25.0",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-sitemap": "^5.25.0",
    "gatsby-plugin-styled-components": "^5.25.0",
    "gatsby-remark-autolink-headers": "^6.13.0",
    "gatsby-remark-images-contentful": "^6.13.0",
    "gatsby-remark-prismjs": "^7.12.0",
    "gatsby-source-contentful": "^2.0.71",
    "gatsby-source-filesystem": "^5.13.0",
    "gatsby-transformer-remark": "^6.13.0",
    "global": "^4.4.0",
    "inquirer": "^9.2.12",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "prismjs": "^1.29.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^6.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "npm run build && gatsby serve",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "setup": "node ./bin/setup.js",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier --list-different {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "prettier": "^3.2.2",
    "stylelint": "16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
